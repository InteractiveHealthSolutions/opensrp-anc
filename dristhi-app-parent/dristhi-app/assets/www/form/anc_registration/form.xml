    <label class="calculation non-select ">
      <input autocomplete="off" type="hidden" name="/ANC_Registration_Pakistan/weight_risk" data-calculate="if( /ANC_Registration_Pakistan/weight_startofpregnancy  &lt; 45 or  /ANC_Registration_Pakistan/weight_startofpregnancy  &gt;= 90, 'Weight', '')" data-type-xml="string"/>
