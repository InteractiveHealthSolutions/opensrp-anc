    <label class="calculation non-select ">
      <input autocomplete="off" type="hidden" name="/ANC_Registration_Pakistan/is_high_risk_till_pnc_close" data-calculate="if( /ANC_Registration_Pakistan/medical_histories  != '', 'yes', 'no')" data-type-xml="string"/>
