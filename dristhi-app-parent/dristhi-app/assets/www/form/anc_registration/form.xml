    <label class="calculation non-select ">
      <input autocomplete="off" type="hidden" name="/ANC_Registration_Pakistan/meta/instanceID" data-calculate="concat('uuid:', uuid())" data-type-xml="string"/>
    </label>
    <label class="calculation non-select ">
      <input autocomplete="off" type="hidden" name="/ANC_Registration_Pakistan/formhub/uuid" data-calculate="'e1edaa4c72b745538d300ae947fe80ab'" data-type-xml="string"/>
    </label>
  </fieldset>
