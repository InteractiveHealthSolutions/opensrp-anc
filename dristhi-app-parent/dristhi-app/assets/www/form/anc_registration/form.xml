    <label class="calculation non-select ">
      <input autocomplete="off" type="hidden" name="/ANC_Registration_Pakistan/height_risk" data-calculate="if( /ANC_Registration_Pakistan/height  &lt;= 145, 'Height', '')" data-type-xml="string"/>
