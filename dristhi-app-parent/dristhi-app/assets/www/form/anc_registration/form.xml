    <label class="calculation non-select ">
      <input autocomplete="off" type="hidden" name="/ANC_Registration_Pakistan/is_high_risk1" data-calculate="if( /ANC_Registration_Pakistan/is_high_risk_till_delivery  = 'yes' or  /ANC_Registration_Pakistan/is_high_risk_till_pnc_close  = 'yes', 'yes', 'no')" data-type-xml="string"/>
