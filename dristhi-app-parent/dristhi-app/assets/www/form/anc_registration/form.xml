    <label class="calculation non-select ">
      <input autocomplete="off" type="hidden" name="/ANC_Registration_Pakistan/is_high_risk_till_delivery" data-calculate="if( /ANC_Registration_Pakistan/case_would_be_high_risk  = 'yes' or  /ANC_Registration_Pakistan/weight_risk  != '' or  /ANC_Registration_Pakistan/height_risk  != '' or  /ANC_Registration_Pakistan/complication  != '', 'yes', 'no')" data-type-xml="string"/>
